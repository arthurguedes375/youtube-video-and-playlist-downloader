{
  "name": "",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "prestart": "clear",
    "start": "node lib/server.js",
    "predev": "clear",
    "dev": "NODE_ENV=dev ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "test": "clear && NODE_ENV=test jest",
    "test:unit": "clear && NODE_ENV=test jest __tests__/unit",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "yarn build:types && yarn build:js && rm -R lib/__tests__",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".js,.ts\" --source-maps inline"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.9",
    "@types/supertest": "^2.0.10",
    "babel-plugin-module-resolver": "^4.0.0",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "morgan": "^1.10.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}